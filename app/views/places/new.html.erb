<h1>New Place</h1>

<%= form_for @place do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :street_address %>
  <%= f.text_field :street_address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.submit  %>
<% end %>
